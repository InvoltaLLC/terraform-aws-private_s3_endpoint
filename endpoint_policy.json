{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "Allow-access-to-specific-bucket",
        "Effect": "Allow",
        "Principal": "*",
        "Action": [
           "s3:ListBucket",
           "s3:GetObject",
           "s3:PutObject"
        ],
        "Resource": [
          "arn:aws:s3:::bucket_name",
          "arn:aws:s3:::bucket_name/*"
        ]
      }
    ]
  }